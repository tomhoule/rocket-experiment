{{#*inline "node"}}
  <h2 id='{{expanded_node.path}}'>
    {{#if expanded_node.node_type.Scope }}
      {{expanded_node.node_type.Scope}}
    {{else}}
      {{expanded_node.node_type}} {{expanded_node.num}}
    {{/if}}
    <a
      data-depth="{{depth}}"
      href="{{fragment_url}}"
    >
      <span class="icon">
        <i class="fa fa-link"></i>
      </span>
    </a>
  </h2>
  {{{rendered}}}
{{/inline}}

{{#*inline "page"}}
<div class="container">
  <div class="content box">
    {{#each schema}}
      {{>node}}
    {{/each}}
  </div>
</div>
{{/inline}}

{{> base}}
